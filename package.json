  },
  "devDependencies": {
    "@jdow/eslint-config": "^0.0.18",
    "@types/node": "^17.0.42",
    "bumpp": "^7.1.1",
    "eslint": "^8.17.0",
    "tsup": "^5.12.9",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
